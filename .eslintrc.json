{
    "extends": [
        "next/core-web-vitals",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "no-console": "warn",
        "no-unused-vars": 1,
        "no-unused-expressions": "warn",
        "no-undef": "warn"
    }
}